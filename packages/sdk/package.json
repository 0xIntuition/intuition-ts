{
  "name": "@0xintuition/sdk",
  "version": "2.0.0",
  "description": "Intuition SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts"
  ],
  "scripts": {
    "build": "pnpm run clean && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "generate": "node ./scripts/generate.mjs",
    "test": "pnpm with-env vitest",
    "with-env": "dotenv -e ../../.env --"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xIntuition/intuition-ts",
    "directory": "packages/sdk"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@0xintuition/graphql": "workspace:*",
    "@0xintuition/protocol": "workspace:*"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@viem/anvil": "^0.0.10",
    "dotenv": "^16.5.0",
    "tsup": "^6.7.0",
    "typescript": "^5.4.5",
    "viem": "^2.31.4",
    "vite": "^5.2.11",
    "vitest": "^1.3.1"
  }
}
