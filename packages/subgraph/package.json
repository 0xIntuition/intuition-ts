{
  "name": "0xintuition/subgraph",
  "type": "module",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy:base": "graph deploy --node https://api.thegraph.com/deploy/ 0xintuition/base",
    "deploy:base-sepolia": "graph deploy --studio intuition-base-sepolia",
    "reset-local": "pnpm remove-local && pnpm create-local && pnpm deploy-local",
    "create-local": "graph create --node http://localhost:8020/ 0xintuition/subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ 0xintuition/subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ -l 1 --ipfs http://localhost:5001 0xintuition/subgraph",
    "prepare:dev": "mustache config/anvil.json subgraph.template.yaml > subgraph.yaml && graph codegen",
    "prepare:base": "mustache config/base.json subgraph.template.yaml > subgraph.yaml && graph codegen",
    "prepare:base-sepolia": "mustache config/base-sepolia.json subgraph.template.yaml > subgraph.yaml && graph codegen",
    "build-and-deploy-prodnets": "pnpm prepare:base && pnpm build && pnpm deploy:base",
    "build-and-deploy-testnets": "pnpm prepare:base-sepolia && pnpm build && pnpm deploy:base-sepolia",
    "update-abi": "jq .abi ../../../intuition-beta-contracts/out/EthMultiVault.sol/EthMultiVault.json > ./abis/EthMultiVault.json",
    "update-bytecode": "echo 'export const bytecode = ' > ./simulation/bytecode.ts && jq .bytecode.object ../../../intuition-beta-contracts/out/EthMultiVault.sol/EthMultiVault.json >> ./simulation/bytecode.ts",
    "dev": "docker-compose down -v && docker-compose up -d && sleep 9 && pnpm codegen && pnpm prepare:dev && pnpm build && pnpm reset-local",
    "simulate:profiles": "tsx simulation/profiles",
    "simulate:books": "tsx simulation/books",
    "simulate:likes": "tsx simulation/likes",
    "simulate:follows": "tsx simulation/follows",
    "simulate:signaling": "tsx simulation/signaling",
    "simulate:pinata": "tsx simulation/signaling-pinata",
    "simulate:json": "tsx simulation/json",
    "simulate:unknown": "tsx simulation/unknown",
    "testnet:deposit": "tsx testnet/deposit",
    "testnet:redeem": "tsx testnet/redeem",
    "testnet:predicates": "tsx testnet/create-predicate-atoms"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.71.2",
    "@graphprotocol/graph-ts": "0.32.0"
  },
  "devDependencies": {
    "@0xintuition/protocol": "workspace:^",
    "@faker-js/faker": "^8.4.1",
    "@pinata/sdk": "^2.1.0",
    "kubo-rpc-client": "^4.1.1",
    "mustache": "^4.2.0",
    "rdf-parse": "^2.3.3",
    "schema-dts": "^1.1.2",
    "tsx": "^4.10.5",
    "viem": "2.9.29"
  }
}
